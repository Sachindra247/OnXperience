{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(static|favicon.ico|manifest.json|logo192.png)(.*)",
      "dest": "/build/$1$2"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
